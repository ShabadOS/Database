        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Then Shiva meditated upon the Shakti (power) and the Power (Shakti) was Gracious towards him.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਸ ਕਰ ਕੇ ਸ਼ਕਤੀ ਨੇ (ਉਸ ਉਤੇ) ਕ੍ਰਿਪਾ ਕੀਤੀ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "ZUUN",
        "source_page": 190,
        "source_line": null,
        "gurmukhi": "qw qy BXo rudR blvwnw ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਸ਼ਿਵ ਬਲਵਾਨ ਹੋ ਗਿਆ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "RE75",
        "source_page": 190,
        "source_line": null,
        "gurmukhi": "mMifXo juDu bhuir ibiD nwnw ]25]",
        "pronunciation": null,
