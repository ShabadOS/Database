        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "A few hours after nightfall, the Khans assembled and advanced for attack.2",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤਾਂ ਪਠਾਣਾਂ ਨੇ ਮਿਲ ਕੇ (ਸਾਡੇ ਉਤੇ) ਚੜ੍ਹਾਈ ਕਰ ਦਿੱਤੀ ॥੨॥",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "D3SP",
        "source_page": 65,
        "source_line": null,
        "gurmukhi": "jb dl pwr ndI ky AwXo ]",
        "pronunciation": null,
