        "source_line": null,
        "gurmukhi": "rwJw hIr imlq jb Bey ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਦੋਂ ਹੀਰ ਅਤੇ ਰਾਂਝਾ ਮਿਲੇ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "9JU9",
        "source_page": 939,
