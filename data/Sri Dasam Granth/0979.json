          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਥੇ ਜੋ ਯੁੱਧ ਹੋਇਆ, ਉਸ ਨੂੰ ਸੁਣਾਉਂਦਾ ਹਾਂ,",
              "additional_information": {}
            }
          }
        }
      },
      {
