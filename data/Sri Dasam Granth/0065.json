        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "When their forces crossed the river, Alam (Singh) came and woke me up.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤਾਂ ਡਿਉਡੀ ਦੇ ਸਰਦਾਰ ਆਲਮ (ਸ਼ਾਹ ਸਿੰਘ) ਨੇ ਸਾਨੂੰ ਜਗਾਇਆ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "GAHG",
        "source_page": 65,
        "source_line": null,
        "gurmukhi": "soru prw sB hI nr jwgy ]",
        "pronunciation": null,
