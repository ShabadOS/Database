        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Bugles of death were sounded and the heroes came laced with the fighting attires and holding spears and bows and arrows.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤਰ੍ਹਾਂ ਤਰ੍ਹਾਂ ਦੇ ਮਾਰੂ ਵਾਜੇ ਵਜਣ ਲਗੇ ਅਤੇ ਯੋਧੇ ਰਣ-ਭੂਮੀ ਵਿਚ ਆ ਕੇ ਡਟ ਗਏ।",
              "additional_information": {}
            }
          }
        }
      },
