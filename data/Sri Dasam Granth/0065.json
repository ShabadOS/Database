        "source_line": null,
        "gurmukhi": "pUq Awpn hm Eir pTwey ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Then Dilwar Khan (Governor of Lahore) sent his son aginst me.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਉਸ ਨੇ ਸਾਰਾ ਬ੍ਰਿੱਤਾਂਤ ਸੁਣ ਕੇ) ਆਪਣੇ ਪੁੱਤਰ ਨੂੰ ਸਾਡੇ ਕੋਲ ਭੇਜਿਆ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "V05E",
        "source_page": 65,
        "source_line": null,
        "gurmukhi": "dÍYk GrI bIqI inis jbY ]",
        "pronunciation": null,
