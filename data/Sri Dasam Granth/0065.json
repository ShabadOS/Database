        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਦੋਂ ਦੋ ਕੁ ਘੜੀਆਂ ਰਾਤ ਬੀਤ ਗਈ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "SG5F",
