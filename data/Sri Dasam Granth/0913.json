        "id": "TWNA",
        "source_page": 913,
        "source_line": null,
        "gurmukhi": "pRwq smY qwrI iqn krI ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਸਵੇਰ ਵੇਲੇ ਉਸ ਨੇ ਤਾੜੀ ਵਜਾਈ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "1HR1",
