          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਕਾਮ ਦੇਵ ਦਾ ਸਾਰਾ ਹੰਕਾਰ ਦੂਰ ਕਰ ਦਿੱਤਾ।",
              "additional_information": {}
            }
          }
        }
