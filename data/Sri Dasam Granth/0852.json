        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "He followed her and found her making love in a derelict house,(28)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਉਸ ਨੂੰ ਸੁੰਨੇ ਘਰ ਵਿਚ ਕਾਮ-ਕ੍ਰੀੜਾ ਕਰਦਿਆਂ ਵੇਖ ਲਿਆ ॥੨੮॥",
              "additional_information": {}
            }
          }
        }
      }
    ]
  },
