          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਰਾਮ ਦਾ ਡਰ ਛੱਡਕੇ (ਉਨ੍ਹਾਂ ਦੇ) ਦਲ ਨੂੰ ਰੋਕ ਲਿਆ ॥੫੨੦॥",
              "additional_information": {}
            }
          }
