        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Therefore he destroyed the demon Jalandhar in and instant  seeing this scene, all were pleased.26.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਯੋਧੇ ਇਹ ਤਮਾਸ਼ਾ ਵੇਖ ਕੇ ਪ੍ਰਸੰਨ ਹੋ ਰਹੇ ਸਨ ॥੨੬॥",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "1NNX",
        "source_page": 190,
        "source_line": null,
        "gurmukhi": "jlµDrI qw idn qy nwmw ]",
        "pronunciation": null,
