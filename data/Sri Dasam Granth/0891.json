        "source_line": null,
        "gurmukhi": "kCU kop nih ihRdY ibcwrhu ]10]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘Now leaving the Shastras come with me, and please don’t get angry.’(10)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਮਨ ਵਿਚ ਜ਼ਰਾ ਜਿੰਨਾ ਗੁੱਸਾ ਨਾ ਕਰੋ ॥੧੦॥",
              "additional_information": {}
            }
          }
        }
      }
    ]
  },
