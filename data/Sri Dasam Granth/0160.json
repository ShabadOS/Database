          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਸਤ੍ਰਾਂ ਦੇ ਪ੍ਰਹਾਰ ਹੋਣ ਲਗ ਗਏ ਸਨ ਅਤੇ ਬਸਤ੍ਰ ਅਤੇ ਚੌਰ ਡਿਗ ਰਹੇ ਸਨ,",
              "additional_information": {}
            }
