          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "The bugles blew, the trumpets resounded, the great heroes entered the fray, shouting loudly.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਬਾਂਕੇ ਤੀਰ-ਅੰਦਾਜ਼ ਅਤੇ ਬਲਵਾਨ ਯੋਧੇ ਲਲਕਾਰੇ ਮਾਰਦੇ (ਯੁੱਧ-ਭੂਮੀ ਵਿਚ ਨਿਤਰੇ)।",
              "additional_information": {}
            }
          }
