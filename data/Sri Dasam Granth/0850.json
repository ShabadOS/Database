        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "The dread of the co-wife always haunted her, and she always craved to annihilate her,",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਸ ਨੂੰ ਧਨ ਦੇਖਣ ਨਹੀਂ ਸੀ ਦਿੰਦੀ,",
              "additional_information": {}
            }
          }
        }
