        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "She would not let money pass to her and plunder it when it was on its way to her (co-wife).(10)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਫਿਰ ਉਹ ਸੌਂਕਣ ਨੂੰ ਜਾ ਕੇ ਮਿਲਦੀ।",
              "additional_information": {}
            }
          }
