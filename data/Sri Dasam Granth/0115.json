        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "“Send Lord Shiva to the place, where the demon-king is standing.”39.195.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਿਥੇ ਦੈਂਤ-ਰਾਜਾ ਖੜੋਤਾ ਹੈ ॥੩੯॥੧੯੫॥",
              "additional_information": {}
            }
          }
        }
      },
      {
