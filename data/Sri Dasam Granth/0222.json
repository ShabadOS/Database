            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਨ ਖੰਡੇ ਜਾ ਸਕਣ ਵਾਲਿਆਂ ਨੂੰ ਖੰਡ ਦਿੱਤਾ,",
              "additional_information": {}
            }
          }
