            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਸ਼ੋਰ ਪੈਣ ਤੇ ਸਭ ਸੈਨਿਕ ਜਾਗ ਗਏ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "R3CS",
        "source_page": 65,
        "source_line": null,
