      {
        "id": "5NN3",
        "source_page": 913,
        "source_line": null,
        "gurmukhi": "mIq nwm suin kMT lgweI ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "When she listened to the letter and hearing the lover’s name she embraced it.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤਾਂ ਮਿਤਰ ਦਾ ਨਾਂ ਸੁਣ ਕੇ ਗਲ ਨਾਲ ਲਾ ਲਈ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "N988",
        "source_page": 913,
        "source_line": null,
        "gurmukhi": "XhY Xwir iliK qwih pTwXo ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਯਾਰ ਨੇ ਉਸ ਨੂੰ ਇਹ ਲਿਖ ਭੇਜਿਆ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "T31M",
        "source_page": 913,
        "source_line": null,
        "gurmukhi": "qum ibnu AiDk kst hm pwXo ]7]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "The lover had expressed that, without her he was in great distress.(7)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਕਿ ਤੇਰੇ ਬਿਨਾ ਮੈਂ ਬਹੁਤ ਦੁਖ ਪਾਇਆ ਹੈ ॥੭॥",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "B3WB",
        "source_page": 913,
        "source_line": null,
        "gurmukhi": "piqXw mY liK XhY pTwXo ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਚਿੱਠੀ ਵਿਚ ਇਹ ਵੀ ਲਿਖ ਭੇਜਿਆ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "3V2X",
        "source_page": 913,
        "source_line": null,
        "gurmukhi": "qum ibn hm sB ikCu ibsrwXo ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "It was mentioned in the letter, ‘I am lost without you,",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਕਿ ਤੇਰੇ ਬਿਨਾ ਮੈਂ ਸਭ ਕੁਝ ਭੁਲਾ ਦਿੱਤਾ ਹੈ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "9422",
        "source_page": 913,
        "source_line": null,
        "gurmukhi": "hmrI suiD Awpn qum lIjhu ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਮੇਰੀ ਸੁਰਤ ਤੂੰ ਆਪ ਲੈ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "2P6P",
        "source_page": 913,
        "source_line": null,
        "gurmukhi": "kCu Dnu kwiF pTY muih dIjhu ]8]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘Now you must take care of me and send me some money to enable me to live.’(8)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਕੁਝ ਧਨ ਕਢ ਕੇ ਮੈਨੂੰ ਭੇਜ ॥੮॥",
              "additional_information": {}
            }
          }
        }
      }
    ]
  },
