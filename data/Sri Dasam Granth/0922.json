        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "They commenced march to the direction of Kamwati.(40)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਕਾਮਵਤੀ ਨਗਰੀ ਵਲ ਤੁਰ ਪਏ ॥੪੦॥",
              "additional_information": {}
            }
          }
        }
      }
