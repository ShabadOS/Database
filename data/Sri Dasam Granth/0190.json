        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Those who repeat the Name of Chandika, they know that from that day, Chnadika came to be known as Jalandhari.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਚੰਡਿਕਾ ਨੂੰ ਸਾਰੇ (ਇਸ ਨਾਮ ਨਾਲੇ) ਅੱਠੇ ਪਹਿਰ ਜਪੋ।",
              "additional_information": {}
            }
          }
        }
      },
      {
