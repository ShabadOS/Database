        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Girded himself for the fight.(11)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਆਪ ਵੀ ਯੁੱਧ ਲਈ ਮਨ ਬਣ ਲਿਆ ॥੧੧॥",
              "additional_information": {}
            }
          }
        }
      }
