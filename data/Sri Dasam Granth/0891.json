        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘Roop Kala was after all not telling lies. I have recognised her truthfulness now.’(12)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਹੁਣ ਮੈਂ ਉਸ ਦੇ ਸਚ ਨੂੰ ਸਮਝ ਲਿਆ ਹੈ ॥੧੨॥",
              "additional_information": {}
            }
          }
        }
      }
