            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਸ ਦਿਨ ਤੋਂ (ਦੁਰਗਾ ਦਾ) ਨਾਂ 'ਜਲੰਧਰੀ' ਪੈ ਗਿਆ।",
              "additional_information": {}
            }
          }
