        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "And observed the various (enemy) armies.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਕਈ ਢੰਗਾਂ ਨਾਲ ਸੈਨਾ ਨੂੰ ਬੇਨਤੀ ਕੀਤੀ (ਅਰਥਾਤ ਯੁੱਧ ਲਈ ਪ੍ਰੇਰਿਤ ਕੀਤਾ)।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "FJ4Q",
        "source_page": 813,
        "source_line": null,
        "gurmukhi": "rn CiqRn ko Awiesu dIno ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
