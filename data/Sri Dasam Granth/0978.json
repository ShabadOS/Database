        "source_line": null,
        "gurmukhi": "nwqr mIc mUMf pr lIjY ]8]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "He asked Rude to handover Parbati to him or accept annihilation.(8)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਨਹੀਂ ਤਾਂ ਮੌਤ ਨੂੰ ਸਿਰ ਉਤੇ ਆਇਆ ਸਮਝੋ ॥੮॥",
              "additional_information": {}
            }
          }
        }
      }
    ]
  },
