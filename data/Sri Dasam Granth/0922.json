            "Dr. Surinder Singh Kohli": {
              "translation": "Bikrim called Madhwan and asked him respectfully to be seated.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਆਦਰ ਪੂਰਵਕ ਆਸਣ ਉਤੇ ਬਿਠਾਇਆ",
