          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "The god Shiva is busy in another dance and on the other side, the Machh incarnation, being pleased, is stirring the ocean.44.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਰੂਦਰ ਭਿਆਨਕ ਰੂਪ ਨਾਲ ਨਚ ਰਿਹਾ ਸੀ ਅਤੇ ਮੱਛ ਸਮੁੰਦਰ ਨੂੰ ਨਚ ਨਚ ਕੇ ਮੱਥ ਰਿਹਾ ਸੀ ॥੪੪॥",
              "additional_information": {}
            }
          }
        }
      }
