        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "All their afflictions were eliminated and they departed for the heavens.(30)",
              "additional_information": {}
