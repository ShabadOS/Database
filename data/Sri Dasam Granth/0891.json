        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘I have seen that happening in your house.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਹੀ ਗੱਲ (ਮੈਂ) ਤੁਹਾਡੇ ਘਰ ਵਿਚ ਵੇਖੀ ਹੈ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "VXLX",
        "source_page": 891,
        "source_line": null,
        "gurmukhi": "Coir ssqR cil qurq inhwrhu ]",
