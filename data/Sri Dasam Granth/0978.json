        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "He was so much fascinated seeing her that he sent a wise emissary to her.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਉਸ ਕੋਲ (ਇਕ) ਚਾਲਾਕ ਦੂਤ ਨੂੰ ਭੇਜਿਆ (ਅਤੇ ਸੁਨੇਹਾ ਘਲਿਆ)।",
              "additional_information": {}
            }
          }
        }
