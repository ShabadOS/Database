      },
      {
        "id": "8174",
        "source_page": 173,
        "source_line": null,
        "gurmukhi": "jh swDn sMkt prY; qh qh Bey shwie ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Wherever the saints are in distress, the Non-temporal Lord comes there for help.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਿਥੇ ਜਿਥੇ ਵੀ ਸਾਧਾਂ ਨੂੰ ਸੰਕਟ ਪੈਂਦਾ ਹੈ, ਉਥੇ ਉਥੇ ਹੀ (ਪਰਮ-ਸੱਤਾ) ਸਹਾਇਕ ਹੁੰਦੀ ਹੈ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "NFJ4",
        "source_page": 173,
        "source_line": null,
        "gurmukhi": "duAwrpwl huAY dir bsy; Bgq hyq hirrwie ]26]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "The Lord, coming under the control of His devotee, became his gate-keeper.26.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਹਰਿ-ਰਾਇ ਭਗਤ ਲਈ ਦੁਆਰਪਾਲ ਹੋ ਕੇ (ਉਸ ਦੇ) ਦਰ ਤੇ ਵਸਦਾ ਹੈ ॥੨੬॥",
              "additional_information": {}
            }
          }
        }
      }
