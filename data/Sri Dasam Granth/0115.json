        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Shiva said to the demon-king, “Listen to my words, the mother of she universe hath said this",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤੈਨੂੰ ਦੁਰਗਾ ਮਾਤਾ ਨੇ ਇਸ ਤਰ੍ਹਾਂ ਕਿਹਾ ਹੈ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "HWHS",
        "source_page": 115,
        "source_line": null,
        "gurmukhi": "dyvn ky dY kY TkurweI ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਕਿ ਜਾਂ ਤਾਂ ਦੇਵਤਿਆਂ ਨੂੰ ਬਾਦਸ਼ਾਹੀ ਦੇ ਦਿਉ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "HAY9",
        "source_page": 115,
        "source_line": null,
