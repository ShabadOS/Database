        "source_line": null,
        "gurmukhi": "dYq rwj ieh bwq n mwnI ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਦੈਂਤ-ਰਾਜੇ ਨੇ ਇਹ ਗੱਲ ਨਾ ਮੰਨੀ।",
              "additional_information": {}
