        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Those persons who adopt different guises are never liked by the men of God.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਨ੍ਹਾਂ (ਭੇਖਧਾਰੀਆਂ ਨੂੰ) ਸੱਚੇ ਸਾਧਕ (ਪ੍ਰਭੂ-ਜਨ) ਕੁਝ ਵੀ ਨਹੀਂ ਸਮਝਦੇ।",
              "additional_information": {}
            }
          }
        }
      },
