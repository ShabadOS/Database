          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਹੇ ਕਵੀ ਜਨੋ! ਕਿਧਰੇ ਕਿਸੇ ਭੁਲ ਵੇਖ ਕੇ ਹਾਸੀ ਨਾ ਕਰਨਾ ॥੨੮॥",
              "additional_information": {}
            }
          }
        }
