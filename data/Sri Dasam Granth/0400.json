          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਪਹਿਲਾਂ ਪੂਤਨਾ ਮਾਰੀ, ਫਿਰ ਸ਼ਕਟਾਸੁਰ ਦਾ ਨਾਸ਼ ਕੀਤਾ।",
              "additional_information": {}
            }
          }
        }
