        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "All of them, dressed in iron-coats, armed themselves",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਸੈਨਿਕਾਂ ਨੇ) ਸ਼ਰੀਰਾਂ ਉਤੇ ਕਵਚ ਅਤੇ ਸ਼ਸਤ੍ਰ ਧਾਰਨ ਕਰ ਲਏ",
              "additional_information": {}
            }
          }
        }
      },
      {
