        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘I will place the bag (containing money) on the wall and, I insist, he must take it away.(11)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਹੇ ਇਸਤਰੀ! ਮੇਰਾ ਕਿਹਾ ਮੰਨ ਲਈਂ ॥੧੧॥",
              "additional_information": {}
            }
          }
        }
      },
