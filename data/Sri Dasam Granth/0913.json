        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "He wrote a letter on behalf of the lover,",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਸ (ਗੁਜਰ) ਨੇ ਉਸ ਦੇ ਮਿਤਰ ਦੇ ਨਾਂ ਤੇ ਇਕ ਚਿੱਠੀ ਲਿਖੀ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "BT7N",
        "source_page": 913,
        "source_line": null,
        "gurmukhi": "eyk AiqQ ko hwQ dY; pTI iqRXw ky Dwm ]6]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "And through a friend sent to the woman.(6)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਇਕ ਸਾਧ ਦੇ ਹੱਥ ਦੇ ਕੇ (ਉਸ) ਇਸਤਰੀ ਦੇ ਘਰ ਭੇਜ ਦਿੱਤੀ ॥੬॥",
              "additional_information": {}
            }
          }
        }
      }
    ]
  },
