        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Ranjha turned into god Indra and Heer became Maneka,",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਥੇ ਰਾਂਝਾ ਇੰਦਰ ਬਣ ਗਿਆ ਅਤੇ ਹੀਰ ਮੈਨਿਕਾ ਬਣ ਗਈ।",
              "additional_information": {}
            }
          }
        }
      },
