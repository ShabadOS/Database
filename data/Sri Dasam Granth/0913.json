        "id": "29PX",
        "source_page": 913,
        "source_line": null,
        "gurmukhi": "qurqu qhw Awpn auiT AYXhu ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘When I will hear the clap with my own ears, I will immediately proceed to the place.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤਾਂ ਤੁਰੰਤ ਆਪ ਉਠ ਕੇ ਉਥੇ ਆ ਜਾਈਂ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "GYEM",
        "source_page": 913,
        "source_line": null,
        "gurmukhi": "kwD aupir kir QYlI lYXhu ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਕੰਧ ਉਪਰ ਥੈਲੀ ਲਿਆ ਕੇ ਰਖ ਦੇਈਂ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "A6RS",
