        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘Why for the sake of a prostitute, get your army killed.’(53)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਇਕ ਨਾਚੀ ਲਈ ਵਿਅਰਥ ਵਿਚ ਸੈਨਾ ਨੂੰ ਕਿਉਂ ਮਰਵਾ ਰਹੇ ਹੋ ॥੫੩॥",
              "additional_information": {}
            }
