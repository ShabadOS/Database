        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "The firm limbs, after swinging continuously, are falling down and the brave and beautiful fighters, fearlessly coming face to face with others are clashing.20.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਸੂਹੇ ਰੰਗ ਵਿੱਚ ਰੰਗੇ ਹੋਏ ਸਾਰੇ ਯੋਧੇ ਨਿਡਰ ਹੋ ਕੇ (ਵੈਰੀ ਨਾਲ) ਜੁਟੇ ਹੋਏ ਸਨ ॥੨੦॥",
              "additional_information": {}
            }
          }
        }
      }
