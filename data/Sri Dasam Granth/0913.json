        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਕਿ ਪ੍ਰਭਾਤ ਵੇਲੇ ਪਿਛਲੇ ਪਾਸੇ ਆਵਾਂਗਾ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "CUM3",
        "source_page": 913,
        "source_line": null,
        "gurmukhi": "duhUM hwQ Bey qwl bjYhO ]10]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘He should come early in morning at the back of the house and clap his hands twice.’(10)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਦੋਹਾਂ ਹੱਥਾਂ ਨਾਲ ਤਾੜੀ ਵਜਾਵਾਂਗਾ ॥੧੦॥",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "8L5H",
        "source_page": 913,
        "source_line": null,
        "gurmukhi": "jb qwrI sRvnn suin pYXhu ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਿਸ ਵੇਲੇ (ਤੂੰ) ਕੰਨਾਂ ਨਾਲ ਤਾੜੀ (ਦੀ ਆਵਾਜ਼) ਸੁਣੇਗੀ",
              "additional_information": {}
            }
          }
        }
      },
