        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "(Madhwan said) ‘Whatever the Brahmin priest orders,",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਕਿਹਾ ਹੇ ਬ੍ਰਾਹਮਣ! ਜੋ ਆਗਿਆ ਦਿਓ, ਉਹੀ ਕਰਾਂਗਾ।",
              "additional_information": {}
            }
          }
        }
      },
      {
