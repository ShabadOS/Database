        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Taking his arrow in his hand, Suraj ran towards the enemy Deeraghkaya and in great ire began a dreadful war.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਯੁੱਧ-ਭੂਮੀ ਵਿੱਚ (ਜਾ ਕੇ) ਕ੍ਰੋਧ ਨਾਲ ਅਨੰਤ ਤਰ੍ਹਾਂ ਦਾ ਯੁੱਧ ਮਚਾਇਆ।",
              "additional_information": {}
            }
          }
        }
      },
