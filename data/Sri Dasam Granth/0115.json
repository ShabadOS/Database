        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "When the female messenger of Shiva heard this, she sent Shiva as a messenger of Shiva",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਤਦ) ਸ਼ਿਵ ਨੂੰ ਦੂਤ ਬਣਾ ਕੇ ਉਥੇ ਭੇਜ ਦਿੱਤਾ।",
              "additional_information": {}
            }
          }
        }
      },
      {
