          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਸ ਦੀ ਉਸਤਤ ਰਾਜੇ ਪਾਸ ਕਰਦੇ ਸਨ।",
              "additional_information": {}
            }
          }
        }
