        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਇਕ ਸੇਵਕਾ) ਉਸ ਰਾਣੀ ਨੂੰ ਸੌਂਕਣ ਦਾ ਬਹੁਤ ਡਰ ਦਿਖਾਂਦੀ ਸੀ",
              "additional_information": {}
            }
          }
        }
