          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਦੂਜਾ ਹੋਰ ਕਿਹੜਾ ਯੋਧਾ ਹੈ ਜੋ ਉਨ੍ਹਾਂ ਨੂੰ ਦਬਾਵੇ ॥੧੧॥",
              "additional_information": {}
            }
          }
        }
