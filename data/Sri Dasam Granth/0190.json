        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "The war continued of several month and Jalndhar was filled with great fury for (the action of ) Shiva.24.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਅੰਤ ਵਿਚ ਜਦੋਂ) ਜਲੰਧਰ ਨੇ ਸ਼ਿਵ ਉਤੇ ਕ੍ਰੋਧ ਕੀਤਾ ॥੨੪॥",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "VEUX",
        "source_page": 190,
        "source_line": null,
        "gurmukhi": "qb isv iDAwn skiq kO Drw ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
