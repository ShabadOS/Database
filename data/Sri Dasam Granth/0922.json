            "Dr. Surinder Singh Kohli": {
              "translation": "‘But I will not be able to give Kamkandla away.’(43)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤਾਂ ਵੀ ਕਾਮਕੰਦਲਾ ਨੂੰ ਦੇਣਾ ਮੇਰੇ ਪਾਸੋਂ (ਸੰਭਵ) ਨਹੀਂ ਹੋਣਾ ॥੪੩॥",
              "additional_information": {}
            }
          }
        }
