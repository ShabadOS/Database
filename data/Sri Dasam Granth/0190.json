        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "By repeating her name, the body becomes pure like taking bath in the Ganges.27.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਿਸ ਤਰ੍ਹਾਂ ਗੰਗਾ ਜਲ ਦੇ ਪ੍ਰਵਾਹ ਵਿਚ ਨ੍ਹਾਤਿਆਂ ਹੋ ਜਾਈਦਾ ਹੈ ॥੨੭॥",
              "additional_information": {}
            }
          }
        }
