        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਇਥੋਂ ਦੀ ਮਿਆਦ ਜਦੋਂ ਪੂਰੀ ਹੋ ਗਈ",
              "additional_information": {}
            }
          }
        }
