          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਰੂਪ ਮਤੀ ਨੇ ਮੈਨੂੰ ਝੂਠ ਨਹੀਂ ਕਿਹਾ ਹੈ।",
              "additional_information": {}
            }
          }
        }
      },
