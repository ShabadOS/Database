              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਦੋਂ ਵੈਰੀ-ਦਲ ਨਦੀ ਤੋਂ ਪਾਰ ਆ ਗਿਆ",
              "additional_information": {}
            }
          }
        }
