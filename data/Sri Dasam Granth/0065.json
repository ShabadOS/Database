        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "There was a great consternation and all the people got up. They took up their arms with valour and zeal.3.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਵੀਰਰਸ ਨਾਲ ਮਤੇ ਹੋਏ ਸ਼ਸਤ੍ਰ ਫੜ ਫੜ ਕੇ (ਅਗੇ ਵਧੇ) ॥੩॥",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "N39S",
        "source_page": 65,
        "source_line": null,
        "gurmukhi": "CUtn lgI quPMgY qbhI ]",
        "pronunciation": null,
        "pronunciation_information": null,
