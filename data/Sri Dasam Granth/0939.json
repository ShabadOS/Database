          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਕਿ ਇਹ ਕੰਨਿਆ ਜੀਉਂਦੀ ਨਹੀਂ ਰਹੇਗੀ।",
              "additional_information": {}
            }
