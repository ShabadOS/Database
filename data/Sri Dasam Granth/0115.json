        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "On this side, Durga after reflecting, called a female messenger of Shiva and making her conscious gave this message in her ear:",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਉਸ ਨੂੰ) ਕੰਨ ਵਿਚ (ਇਹ ਗੱਲ) ਚੰਗੀ ਤਰ੍ਹਾਂ ਸਮਝਾਈ",
              "additional_information": {}
            }
