          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਇਸਤਰੀ ਦਾ ਰੂਪ ਵੇਖ ਕੇ (ਜਲੰਧਰ) ਲਲਚਾ ਗਿਆ",
              "additional_information": {}
            }
