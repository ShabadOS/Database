        "id": "F1KN",
        "source_page": 939,
        "source_line": null,
        "gurmukhi": "Xw jg mY gwvq sdw; sB kib kul js DIr ]31]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "And all the revered poets sang the songs in their praise.(31)(1)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਇਸ ਜਗਤ ਵਿਚ ਸਾਰੇ ਕਵੀ-ਕੁਲ (ਇਨ੍ਹਾਂ ਦਾ) ਸਦਾ ਧੀਰਜ ਨਾਲ ਜਸ ਗਾਉਂਦੇ ਹਨ ॥੩੧॥",
              "additional_information": {}
            }
          }
        }
      },
