        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘Now I will narrate you the fighting which went on here, which would sooth your feeling.’(25)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਿਸ ਕਰ ਕੇ ਤੁਹਾਡੇ ਹਿਰਦੇ ਨੂੰ ਪ੍ਰਸੰਨ ਕਰਦਾ ਹਾਂ ॥੨੫॥",
              "additional_information": {}
            }
          }
        }
      }
