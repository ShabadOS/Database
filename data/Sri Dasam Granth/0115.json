        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "“That either you return the kingdom to gods or wage the war with me.”41.197.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਾਂ ਫਿਰ ਮੇਰੇ ਨਾਲ ਯੁੱਧ ਕਰੋ ॥੪੧॥੧੯੭॥",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "CMXR",
