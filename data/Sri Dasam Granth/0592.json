        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "In order to kill Kalki, the warriors cautiously marched forward and began to wage war here, there and everywhere",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਿਥੇ ਕਿਥੇ ਯੁੱਧ ਦੇ ਵਿਧਾਨ ਹੋਣ ਲਗੇ ਹਨ।",
              "additional_information": {}
