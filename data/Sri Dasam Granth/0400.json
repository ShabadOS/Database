        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Krishna killed the demon named Aghasura",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਕੇਸੀ, ਬੱਛ ਅਤੇ ਧੇਨਕ (ਨਾਂ ਦੇ ਦੈਂਤਾਂ) ਨੂੰ ਮਾਰ ਦਿੱਤਾ ਸੀ",
              "additional_information": {}
            }
          }
        }
      },
      {
