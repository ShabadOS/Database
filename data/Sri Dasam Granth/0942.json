          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਇੰਦਰ ਨੇ) ਦੂਤ ਨੂੰ ਕਿਹਾ ਕਿ ਤੂੰ ਤੁਰਤ ਜਾ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "4V8M",
        "source_page": 942,
        "source_line": null,
        "gurmukhi": "sYn sihq dsrQ kY lRXYXhu ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "He told his ambassadors, ‘Go and get Dasrath,",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਸੈਨਾ ਸਹਿਤ ਦਸ਼ਰਥ ਨੂੰ ਲੈ ਆ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "5BFF",
        "source_page": 942,
        "source_line": null,
        "gurmukhi": "igRh ky skl kwm qj AwvY ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਉਹ) ਘਰ ਦੇ ਸਾਰੇ ਕੰਮ ਛਡ ਕੇ ਆ ਜਾਵੇ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "W0ZK",
        "source_page": 942,
        "source_line": null,
        "gurmukhi": "hmrI idis hÍY juDu mcwvY ]3]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘And tell him to come abandoning all his tasks and to go to fight on our behalf.’(3)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਸਾਡੇ ਵਲੋਂ ਹੋ ਕੇ ਯੁੱਧ ਕਰੇ ॥੩॥",
              "additional_information": {}
            }
          }
        }
      }
    ]
  },
