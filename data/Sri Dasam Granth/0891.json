        "source_page": 891,
        "source_line": null,
        "gurmukhi": "nwaU ky iek puqR so qw ko rhY pÎwr ]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "He esteemed the son of a barber,",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਸ ਦਾ ਪਿਆਰ ਇਕ ਨਾਈ ਦੇ ਪੁੱਤਰ ਨਾਲ ਸੀ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "2C5X",
        "source_page": 891,
        "source_line": null,
        "gurmukhi": "sUriq mY doaU eykso; koaU n skY ibcwr ]2]",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "And they looked alike so much that no one could distinguish.(2)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਸ਼ਕਲੋਂ ਦੋਵੇਂ ਇਕੋ ਜਿਹੇ ਸਨ। (ਉਨ੍ਹਾਂ ਨੂੰ) ਕੋਈ ਪਛਾਣ ਨਹੀਂ ਸਕਦਾ ਸੀ ॥੨॥",
              "additional_information": {}
            }
          }
