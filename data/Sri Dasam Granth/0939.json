        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "During his begging, when he found opportunity, he took Heer and departed for the domain of death.(29)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤਾਂ ਉਹ ਉਸ ਨੂੰ ਲੈ ਕੇ ਸਵਰਗ ਚਲਿਆ ਗਿਆ ॥੨੯॥",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "LEYE",
