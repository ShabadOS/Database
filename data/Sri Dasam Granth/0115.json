        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਸ਼ਿਵ-ਦੂਤੀ ਨੇ ਜਦ ਇਹ ਸੁਣਿਆ",
              "additional_information": {}
            }
          }
        }
      },
